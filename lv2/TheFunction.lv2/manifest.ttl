@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .

<http://arcticanaudio.com/plugins/thefunction>
    a lv2:Plugin ;
    lv2:binary <TheFunction.so> ;
    rdfs:seeAlso <TheFunction.ttl> .

<http://arcticanaudio.com/plugins/thefunction#ExternalUI>
    a <http://kxstudio.sf.net/ns/lv2ext/external-ui#Widget> ;
    ui:binary <TheFunction.so> ;
    lv2:requiredFeature <http://lv2plug.in/ns/ext/instance-access> ;
    lv2:extensionData <http://kxstudio.sf.net/ns/lv2ext/programs#UIInterface> .

<http://arcticanaudio.com/plugins/thefunction#ParentUI>
    a ui:X11UI ;
    ui:binary <TheFunction.so> ;
    lv2:requiredFeature <http://lv2plug.in/ns/ext/instance-access> ;
    lv2:optionalFeature ui:noUserResize ;
    lv2:extensionData <http://kxstudio.sf.net/ns/lv2ext/programs#UIInterface> .

<http://arcticanaudio.com/plugins/thefunction#preset001>
    a pset:Preset ;
    lv2:appliesTo <http://arcticanaudio.com/plugins/thefunction> ;
    rdfs:label "Blank Canvas" ;
    rdfs:seeAlso <presets.ttl> .

<http://arcticanaudio.com/plugins/thefunction#preset002>
    a pset:Preset ;
    lv2:appliesTo <http://arcticanaudio.com/plugins/thefunction> ;
    rdfs:label "Phased" ;
    rdfs:seeAlso <presets.ttl> .

<http://arcticanaudio.com/plugins/thefunction#preset003>
    a pset:Preset ;
    lv2:appliesTo <http://arcticanaudio.com/plugins/thefunction> ;
    rdfs:label "Stereo->Mono" ;
    rdfs:seeAlso <presets.ttl> .

<http://arcticanaudio.com/plugins/thefunction#preset004>
    a pset:Preset ;
    lv2:appliesTo <http://arcticanaudio.com/plugins/thefunction> ;
    rdfs:label "Mono Blocker" ;
    rdfs:seeAlso <presets.ttl> .

<http://arcticanaudio.com/plugins/thefunction#preset005>
    a pset:Preset ;
    lv2:appliesTo <http://arcticanaudio.com/plugins/thefunction> ;
    rdfs:label "Gain Reduction" ;
    rdfs:seeAlso <presets.ttl> .


