@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://zynaddsubfx.sourceforge.net/fx#Phaser>
    a lv2:PhaserPlugin, lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "LFO Frequency" ;
        lv2:symbol "lfofreq" ;
        lv2:default 36 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "LFO Randomness" ;
        lv2:symbol "lforand" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "LFO Type" ;
        lv2:symbol "lfotype" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "LFO Stereo" ;
        lv2:symbol "lfostereo" ;
        lv2:default 64 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Depth" ;
        lv2:symbol "depth" ;
        lv2:default 110 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Feedback" ;
        lv2:symbol "fb" ;
        lv2:default 64 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "Stages" ;
        lv2:symbol "stages" ;
        lv2:default 1 ;
        lv2:minimum 1 ;
        lv2:maximum 12 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "L/R Cross|Offset" ;
        lv2:symbol "lrcross" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 12 ;
        lv2:name "Subtract Output" ;
        lv2:symbol "subsout" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 13 ;
        lv2:name "Phase|Width" ;
        lv2:symbol "phase" ;
        lv2:default 20 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 14 ;
        lv2:name "Hyper" ;
        lv2:symbol "hyper" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 15 ;
        lv2:name "Distortion" ;
        lv2:symbol "dist" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 16 ;
        lv2:name "Analog" ;
        lv2:symbol "analog" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] ;

    mod:brand "ZynAddSubFX" ;
    mod:label "ZynPhaser" ;

    doap:name "ZynPhaser" ;
    doap:license "GPL v2" ;

    doap:maintainer [
        foaf:name "ZynAddSubFX Team" ;
        foaf:homepage <http://zynaddsubfx.sourceforge.net> ;
    ] ;

    lv2:microVersion 5 ;
    lv2:minorVersion 5 .

