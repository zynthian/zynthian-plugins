@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://distrho.sf.net/plugins/MVerb>
    a lv2:ReverbPlugin, lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    ui:ui <http://distrho.sf.net/plugins/MVerb#UI> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Damping" ;
        lv2:symbol "damping" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Density" ;
        lv2:symbol "density" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Bandwidth" ;
        lv2:symbol "bandwidth" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Decay" ;
        lv2:symbol "decay" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Predelay" ;
        lv2:symbol "predelay" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Size" ;
        lv2:symbol "size" ;
        lv2:default 75.000000 ;
        lv2:minimum 5.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "Gain" ;
        lv2:symbol "gain" ;
        lv2:default 100.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "Mix" ;
        lv2:symbol "mix" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 12 ;
        lv2:name "Early/Late Mix" ;
        lv2:symbol "earlymix" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        unit:unit unit:pc ;
    ] ;

    rdfs:comment """
Studio quality reverb, provides a practical demonstration of Dattorroâ€™s figure-of-eight reverb structure.
""" ;

    mod:brand "DISTRHO" ;
    mod:label "MVerb" ;

    doap:name "MVerb" ;
    doap:license "GPL v3+" ;

    doap:maintainer [
        foaf:name "Martin Eastwood, falkTX" ;
        foaf:homepage <https://github.com/DISTRHO/MVerb> ;
    ] ;

    lv2:microVersion 0 ;
    lv2:minorVersion 2 .

