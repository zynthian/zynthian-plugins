@prefix doap: 	<http://usefulinc.com/ns/doap#> .
@prefix foaf: 	<http://xmlns.com/foaf/0.1/> .
@prefix lv2: 	<http://lv2plug.in/ns/lv2core#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: 	<http://ll-plugins.nongnu.org/lv2/namespace#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .

<http://avwlv2.sourceforge.net/plugins/avw/granulator_stereo/gui>
  	a guiext:GtkUI ;
  	guiext:binary <granulator_stereo_gui.so> .

<http://avwlv2.sourceforge.net/plugins/avw/granulator_stereo>
	a lv2:Plugin, lv2:FilterPlugin ;
	lv2:project <http://avwlv2.sourceforge.net/plugins/avw/> ;        
	doap:name "Granulator - Stereo" ;
	doap:license <http://opensource.org/licenses/isc> ;
	ll:pegName "p" ;
	guiext:ui <http://avwlv2.sourceforge.net/plugins/avw/granulator_stereo/gui> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort, lv2:AudioPort ;
		lv2:index 0 ;
		lv2:symbol "inputL" ;
		lv2:name "Input - L" ;
	] , [
		a lv2:InputPort, lv2:AudioPort ;
		lv2:index 1 ;
		lv2:symbol "inputR" ;
		lv2:name "Input - R" ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "inputGain" ;
		lv2:name "Input Gain" ; 
		lv2:default 1 ;
		lv2:minimum -10 ;
		lv2:maximum 10 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "grainSize" ;
		lv2:name "Grain Size (ms)" ; 
		lv2:default 10 ;
		lv2:minimum 6 ;
		lv2:maximum 10000 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "attack" ;
		lv2:name "Attack (ms)" ; 
		lv2:default 4 ;
		lv2:minimum 3 ;
		lv2:maximum 100 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "release" ;
		lv2:name "Release (ms)" ; 
		lv2:default 4 ;
		lv2:minimum 3 ;
		lv2:maximum 100 ;
	] , [
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "grainDensity" ;
		lv2:name "Grain Spacing (ms)" ; 
		lv2:default 10 ;
		lv2:minimum 1 ;
		lv2:maximum 10000 ;
	] , [
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "grainSpread" ;
		lv2:name "Grain Spread" ; 
		lv2:default 100 ;
		lv2:minimum 2 ;
		lv2:maximum 10000 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "outputGain" ;
		lv2:name "Output Gain" ; 
		lv2:default 1 ;
		lv2:minimum -10 ;
		lv2:maximum 10 ;
	] ,	[
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 9 ;
		lv2:symbol "outputL" ;
		lv2:name "Output - L" ;
	] ,	[
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 10 ;
		lv2:symbol "outputR" ;
		lv2:name "Output - R" ;
	].
