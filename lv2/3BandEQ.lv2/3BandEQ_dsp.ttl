@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://distrho.sf.net/plugins/3BandEQ>
    a lv2:EQPlugin, lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    ui:ui <http://distrho.sf.net/plugins/3BandEQ#UI> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Low" ;
        lv2:symbol "low" ;
        lv2:default 0.000000 ;
        lv2:minimum -24.000000 ;
        lv2:maximum 24.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Mid" ;
        lv2:symbol "mid" ;
        lv2:default 0.000000 ;
        lv2:minimum -24.000000 ;
        lv2:maximum 24.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "High" ;
        lv2:symbol "high" ;
        lv2:default 0.000000 ;
        lv2:minimum -24.000000 ;
        lv2:maximum 24.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Master" ;
        lv2:symbol "master" ;
        lv2:default 0.000000 ;
        lv2:minimum -24.000000 ;
        lv2:maximum 24.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Low-Mid Freq" ;
        lv2:symbol "low_mid" ;
        lv2:default 220.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1000.000000 ;
        unit:unit unit:hz ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Mid-High Freq" ;
        lv2:symbol "mid_high" ;
        lv2:default 2000.000000 ;
        lv2:minimum 1000.000000 ;
        lv2:maximum 20000.000000 ;
        unit:unit unit:hz ;
    ] ;

    rdfs:comment """
3 Band Equalizer, stereo version.
""" ;

    mod:brand "DISTRHO" ;
    mod:label "3 Band EQ" ;

    doap:name "3 Band EQ" ;
    doap:license "LGPL" ;

    doap:maintainer [
        foaf:name "DISTRHO" ;
        foaf:homepage <https://github.com/DISTRHO/Mini-Series> ;
    ] ;

    lv2:microVersion 0 ;
    lv2:minorVersion 2 .

