        lv2:minimum 1.00000 ;
        lv2:maximum 64.0000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "knee" ;
        lv2:name "Knee" ;
        lv2:portProperty epp:hasStrictBounds ;
