:in a pg:MonoGroup , pg:InputGroup ;
    lv2:symbol "in" ;
    rdfs:label "Input" .

<http://calf.sourceforge.net/plugins/MonoCompressor> a lv2:Plugin ;
    a lv2:CompressorPlugin ;
    doap:name "Calf Mono Compressor" ;
    doap:maintainer [ foaf:name "Damien Zammit" ; ] ;
    uiext:ui <http://calf.sourceforge.net/plugins/gui/gtk2-gui> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/instance-access> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/data-access> ;
    doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
    dct:replaces <urn:ladspa:34167> ;
    lv2:optionalFeature epp:supportsStrictBounds ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput :in ;
    lv2:port [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "In L" ;
        lv2:designation pg:left ;
        pg:group :in ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "out_l" ;
        lv2:name "Out L" ;
        lv2:designation pg:left ;
        pg:group :out ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "bypass" ;
        lv2:name "Bypass" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "level_in" ;
        lv2:name "Input" ;
        lv2:default 1.00000 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64.0000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "meter_in" ;
        lv2:name "Input" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "meter_out" ;
        lv2:name "Output" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "clip_in" ;
        lv2:name "0dB-In" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:portProperty lv2:toggled ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "clip_out" ;
        lv2:name "0dB-Out" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:portProperty lv2:toggled ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "threshold" ;
        lv2:name "Threshold" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.125000 ;
        lv2:minimum 0.000976563 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "ratio" ;
        lv2:name "Ratio" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 2.00000 ;
        lv2:minimum 1.00000 ;
        lv2:maximum 20.0000 ;
        ue:unit ue:coef ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "attack" ;
        lv2:name "Attack" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 20.0000 ;
        lv2:minimum 0.0100000 ;
        lv2:maximum 2000.00 ;
        ue:unit ue:ms ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "release" ;
        lv2:name "Release" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 250.000 ;
        lv2:minimum 0.0100000 ;
        lv2:maximum 2000.00 ;
        ue:unit ue:ms ;
    ] , [
        a lv2:InputPort ;
