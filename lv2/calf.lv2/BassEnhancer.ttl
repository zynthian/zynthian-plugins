:in a pg:StereoGroup , pg:InputGroup ;
    lv2:symbol "in" ;
    rdfs:label "Input" .

:out a pg:StereoGroup , pg:OutputGroup ;
    lv2:symbol "out" ;
    rdfs:label "Output" .

<http://calf.sourceforge.net/plugins/BassEnhancer> a lv2:Plugin ;
    a lv2:SpectralPlugin ;
    doap:name "Calf Bass Enhancer" ;
    doap:maintainer [ foaf:name "Markus Schmidt / Krzysztof Foltman" ; ] ;
    uiext:ui <http://calf.sourceforge.net/plugins/gui/gtk2-gui> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/instance-access> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/data-access> ;
    doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
    dct:replaces <urn:ladspa:34098> ;
    lv2:optionalFeature epp:supportsStrictBounds ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput :in ;
    pg:mainOutput :out ;
    lv2:port [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "In L" ;
        lv2:designation pg:left ;
        pg:group :in ;
    ] , [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "in_r" ;
        lv2:name "In R" ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:designation pg:right ;
        pg:group :in ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "out_l" ;
        lv2:name "Out L" ;
        lv2:designation pg:left ;
        pg:group :out ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "out_r" ;
        lv2:name "Out R" ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:designation pg:right ;
        pg:group :out ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "bypass" ;
        lv2:name "Bypass" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "level_in" ;
        lv2:name "Input" ;
        lv2:default 1.00000 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64.0000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "level_out" ;
        lv2:name "Output" ;
        lv2:default 1.00000 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64.0000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "amount" ;
        lv2:name "Amount" ;
        lv2:default 1.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 64.0000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "meter_in" ;
        lv2:name "Input" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "meter_out" ;
        lv2:name "Output" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "clip_in" ;
        lv2:name "0dB" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "clip_out" ;
        lv2:name "0dB" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "drive" ;
        lv2:name "Harmonics" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 8.50000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 10.0000 ;
        ue:unit ue:coef ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "blend" ;
        lv2:name "Blend harmonics" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.00000 ;
        lv2:minimum -10.0000 ;
        lv2:maximum 10.0000 ;
        ue:unit ue:coef ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "meter_drive" ;
        lv2:name "Harmonics level" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "freq" ;
        lv2:name "Scope" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 100.000 ;
        lv2:minimum 10.0000 ;
        lv2:maximum 250.000 ;
        ue:unit ue:hz ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 16 ;
        lv2:symbol "listen" ;
        lv2:name "Listen" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 17 ;
        lv2:symbol "floor_active" ;
        lv2:name "Floor active" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 18 ;
        lv2:symbol "floor" ;
        lv2:name "Floor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 20.0000 ;
        lv2:minimum 10.0000 ;
        lv2:maximum 120.000 ;
        ue:unit ue:hz ;
    ]
.


