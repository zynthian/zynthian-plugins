@prefix : <http://calf.sourceforge.net/plugins/VintageDelay#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix uiext: <http://lv2plug.in/ns/extensions/ui#> .
@prefix lv2ev: <http://lv2plug.in/ns/ext/event#> .
@prefix lv2midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix lv2ctx: <http://lv2plug.in/ns/dev/contexts#> .
@prefix strport: <http://lv2plug.in/ns/dev/string-port#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix ue: <http://lv2plug.in/ns/extensions/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .

<http://calf.sourceforge.net/plugins/gui/gtk2-gui>
    a uiext:GtkUI ;
    lv2:extensionData uiext:showInterface ;
    lv2:requiredFeature uiext:makeResident ;
    uiext:binary <calflv2gui.so> .

:in a pg:StereoGroup , pg:InputGroup ;
    lv2:symbol "in" ;
    rdfs:label "Input" .

:out a pg:StereoGroup , pg:OutputGroup ;
    lv2:symbol "out" ;
    rdfs:label "Output" .

<http://calf.sourceforge.net/plugins/VintageDelay> a lv2:Plugin ;
    a lv2:DelayPlugin ;
    doap:name "Calf Vintage Delay" ;
    doap:maintainer [ foaf:name "Krzysztof Foltman" ; ] ;
    uiext:ui <http://calf.sourceforge.net/plugins/gui/gtk2-gui> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/instance-access> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/data-access> ;
    doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
    dct:replaces <urn:ladspa:33922> ;
    lv2:optionalFeature epp:supportsStrictBounds ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput :in ;
    pg:mainOutput :out ;
    lv2:port [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "In L" ;
        lv2:designation pg:left ;
        pg:group :in ;
    ] , [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "in_r" ;
        lv2:name "In R" ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:designation pg:right ;
        pg:group :in ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "out_l" ;
        lv2:name "Out L" ;
        lv2:designation pg:left ;
        pg:group :out ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "out_r" ;
        lv2:name "Out R" ;
        lv2:designation pg:right ;
        pg:group :out ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "bpm" ;
        lv2:name "Tempo" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 120.000 ;
        lv2:minimum 30.0000 ;
        lv2:maximum 300.000 ;
        ue:unit ue:bpm ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "bpm_host" ;
        lv2:name "Host BPM" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 120.000 ;
        lv2:minimum 1.00000 ;
        lv2:maximum 300.000 ;
        ue:unit ue:bpm ;
        lv2:designation <http://lv2plug.in/ns/ext/time#beatsPerMinute> ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "subdiv" ;
        lv2:name "Subdivide" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:default 4.00000 ;
        lv2:minimum 1.00000 ;
        lv2:maximum 16.0000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "time_l" ;
        lv2:name "Time L" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:default 3.00000 ;
        lv2:minimum 1.00000 ;
        lv2:maximum 16.0000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "time_r" ;
        lv2:name "Time R" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:default 5.00000 ;
        lv2:minimum 1.00000 ;
        lv2:maximum 16.0000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "feedback" ;
        lv2:name "Feedback" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.500000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "amount" ;
        lv2:name "Amount" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.250000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 4.00000 ;
        ue:unit ue:coef ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "mix_mode" ;
        lv2:name "Mix mode" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [ rdfs:label "Stereo"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "Ping-Pong"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "L then R"; rdf:value 2 ] ;
        lv2:scalePoint [ rdfs:label "R then L"; rdf:value 3 ] ;
        lv2:default 1.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 3.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "medium" ;
        lv2:name "Medium" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [ rdfs:label "Plain"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "Tape"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "Old Tape"; rdf:value 2 ] ;
        lv2:default 1.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 2.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "dry" ;
        lv2:name "Dry Amount" ;
        lv2:default 1.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 4.00000 ;
        ue:unit ue:coef ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "width" ;
        lv2:name "Stereo Width" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 1.00000 ;
        lv2:minimum -1.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "sync" ;
        lv2:name "Sync BPM" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ]
.


