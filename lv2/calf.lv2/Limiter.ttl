        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "meter_outL" ;
        lv2:name "Meter-OutL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "meter_outR" ;
        lv2:name "Meter-OutR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
        ue:unit ue:db ;
    ] , [
