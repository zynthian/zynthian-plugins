@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .

<https://github.com/laixinyuan/StereoSourceSepartion>
    a lv2:Plugin ;
    lv2:requiredFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;
    lv2:extensionData <http://lv2plug.in/ns/ext/options#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    ui:ui <https://github.com/laixinyuan/StereoSourceSepartion#ExternalUI> ,
          <https://github.com/laixinyuan/StereoSourceSepartion#ParentUI> ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_freewheel" ;
        lv2:name "Freewheel" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:designation <http://lv2plug.in/ns/lv2core#freeWheeling> ;
        lv2:portProperty lv2:toggled, <http://lv2plug.in/ns/ext/port-props#notOnGUI> ;
    ] ,
    [
        a lv2:OutputPort, lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_latency" ;
        lv2:name "Latency" ;
        lv2:designation <http://lv2plug.in/ns/lv2core#latency> ;
        lv2:portProperty lv2:reportsLatency, lv2:integer ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 4 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 5 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "status" ;
        lv2:name "Status" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "direction" ;
        lv2:name "Direction" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "width" ;
        lv2:name "Width" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "filtertype" ;
        lv2:name "FilterType" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "cutofffrequency" ;
        lv2:name "CutOffFrequency" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] ;

    doap:name "StereoSourceSeparation" ;
    doap:maintainer [ foaf:name "AnnieShin" ] .

