@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://zynaddsubfx.sourceforge.net/fx#Reverb>
    a lv2:ReverbPlugin, lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Time" ;
        lv2:symbol "time" ;
        lv2:default 63 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Delay" ;
        lv2:symbol "delay" ;
        lv2:default 24 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Feedback" ;
        lv2:symbol "fb" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "bw (unused)" ;
        lv2:symbol "unused_bw" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "E/R (unused)" ;
        lv2:symbol "unused_er" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Low-Pass Filter" ;
        lv2:symbol "lpf" ;
        lv2:default 85 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "High-Pass Filter" ;
        lv2:symbol "hpf" ;
        lv2:default 5 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "Damp" ;
        lv2:symbol "damp" ;
        lv2:default 83 ;
        lv2:minimum 64 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 12 ;
        lv2:name "Type" ;
        lv2:symbol "type" ;
        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 2 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 13 ;
        lv2:name "Room size" ;
        lv2:symbol "size" ;
        lv2:default 64 ;
        lv2:minimum 1 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 14 ;
        lv2:name "Bandwidth" ;
        lv2:symbol "bw" ;
        lv2:default 20 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ;

    mod:brand "ZynAddSubFX" ;
    mod:label "ZynReverb" ;

    doap:name "ZynReverb" ;
    doap:license "GPL v2" ;

    doap:maintainer [
        foaf:name "ZynAddSubFX Team" ;
        foaf:homepage <http://zynaddsubfx.sourceforge.net> ;
    ] ;

    lv2:microVersion 5 ;
    lv2:minorVersion 5 .

